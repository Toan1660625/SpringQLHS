<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
                                 http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd" version="2.1">

    <!-- JPA Named Native Queries -->
    <named-native-query name="User.findAll" result-class="com.example.entity.User">
        <query>SELECT * FROM User</query>
    </named-native-query>
    <named-native-query name="User.findById" result-class="com.example.entity.User">
        <query>SELECT * FROM User WHERE userId = :id</query>
    </named-native-query>


</entity-mappings>